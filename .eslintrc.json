{
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2017,
        "ecmaFeatures": {
            "jsx": true,
            "impliedStrict": true
        }
    },
    "extends": "airbnb",
    "rules": {
      "no-console": 0,
      "no-underscore-dangle": 0
    }
  }